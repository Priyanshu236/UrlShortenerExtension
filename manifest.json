{
    "name": "Url Shortener",
    "description": "Shortens the url",
    "version": "1.0",
    "manifest_version": 3,
    "icons":{
        "128":"download.png",
        "64":"clip_icon.png"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
      },
      "permissions": [
        "storage"
      ]
  }